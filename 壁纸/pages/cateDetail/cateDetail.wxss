.fiexd{
  width: 100%;
  height: 80rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9;
}

.sy{
  width: 100%;
  padding: 0 30rpx;
  height: 80rpx;
  background: #fff;
  overflow-x: auto; 
  white-space:nowrap;
  overflow: hidden;
  box-shadow: 0 0 8rpx #999;
}

.nav-item{
  display: inline-block;
  padding: 10rpx 30rpx;
  padding-left: 0;
  margin-right: 50rpx;
  color: #999;
  font-size: 32rpx;
  text-align: center;
  line-height: 60rpx;
}

.item-act{
  font-weight: bold;
  color: #333;
}

/* 空白 */
.kb{
  height: 80rpx;
}

/* 外层容器 */
.warp{
  width: 100%;
  padding: 30rpx;
}

/* 图片 */
.warp .list-img{
  width: 210rpx;
  margin-right: 30rpx;
  height: 385rpx;
  margin-bottom: 30rpx;
}

/* 逢3 去掉右边距 */
.warp .list-img:nth-of-type(3n){
  margin-right: 0;
}