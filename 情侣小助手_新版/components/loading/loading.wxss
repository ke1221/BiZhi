.loading-text {
    margin-top: 100rpx;
    padding: 30rpx 0;
    width: 100%;
    line-height: 40rpx;
    font-size: 28rpx;
    color: #9B9B9B;
    text-align: center;
}

.loading-none {
    margin-top: 114rpx;
    padding-top: 190rpx;
    padding-bottom: 40rpx;
    text-align: center;
    font-size: 28rpx;
    color: #B9B9B9;
    background: url(data:image/jpeg;base64,) center 0 no-repeat;
    background-size: 126rpx;
}

.loading-done {
    margin-top: 60rpx;
    padding-top: 140rpx;
    background: url(data:image/png;base64,) no-repeat center 0;
    background-size: 211rpx;
}

.loading-on span {
    position: relative;
    padding-left: 20rpx;
}

.loading-on-girl {
    width: 64rpx;
    height: 98rpx;
    position: absolute;
    top: -50rpx;
    left: -64rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}