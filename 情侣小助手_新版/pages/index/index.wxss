page {
  height: 100vh;
  overflow: hidden;
}

.index-bg {
  width: 100%;
  height: 100vh;
  position: fixed;
  z-index: -1;
  top: 0;
  left: 0;
}

.index {
  width: 750rpx;
  height: 100%;
  transition: all 1s;
}

.one {
  height: 100%;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
}

.index-top {
  flex: 1;
  width: 100%;
}

.index-conter {
  width: 100%;
  height: 340rpx;
  display: flex;
  align-items: stretch;
  position: relative;
  justify-content: space-between;
}

.index-conter-bg {
  position: absolute;
  display: block;
  width: 100%;
  height: 160rpx;
  bottom: -30rpx;
  z-index: -1;
}

.conter-left {
  display: flex;
  align-items: center;
  justify-content: flex-start;
}

.conter-left-left {
  width: 70rpx;
  height: 280rpx;
}

.conter-left-right {
  display: flex;
  flex-direction: column;
}

.left-right-top {
  /* text-align: center; */
  display: flex;
  flex-direction: column;
  /* align-items: center; */
  justify-content: center;
  margin-left: 50rpx;
}

.right-top-t {
  font-size: 42rpx;
  font-weight: bold;
  color: #fff;
}

.right-top-t text {
  font-size: 22rpx;
  font-weight: 400;
}

.right-top-bot {
  font-size: 22rpx;
  height: 60rpx;
  color: #fff;
}

.left-right-bot {
  display: flex;
  align-items: center;
  justify-content: center;
}

.right-bot-left, .right-bot-right {
	overflow: hidden;
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
}

.right-bot-right {
  position: relative;
}
.right-bot-right-btn {
  right: 15rpx;
  padding: 0;
}
.right-bot-right-img {
  width: 110rpx;
  height: 110rpx;
}
.conter-right {
  width: 246rpx;
}

.conter-right-item {
  width: 100%;
  height: 92rpx;
  padding: 4rpx;
  margin-top: 20rpx;
  border-top-left-radius: 46rpx;
  border-bottom-left-radius: 46rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  background-color: rgba(255,255,255,0.5);
}

.right-item-icon {
  width: 88rpx;
  height: 88rpx;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #fff;
}

.item-icon {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}

.right-item-right {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: stretch;
  text-align: center;
  justify-content: center;
}

.item-right-num {
  font-size: 42rpx;
  font-weight: bold;
  color: #fff;
}

.item-right-text {
  font-size: 22rpx;
  color: #fff;
}

.index-bottom {
  width: 100%;
  background: #FF6CA4;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.bottom-left {
  flex: 1;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  padding: 30rpx;
}

.bottom-left-top {
  display: flex;
  align-items: center;
  justify-content: flex-start;
}

.bottom-left-top-item {
  width: 285rpx;
  height: 120rpx;
  background: #fff;
  border-radius: 8rpx;
  padding: 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.item-l {
  margin-right: 18rpx;
}

.bottom-item-icon {
  width: 58rpx;
  height: 58rpx;
}

.bottom-item-text {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #FF6CA4;
  font-size: 24rpx;
}

.bottom-item-text-title {
  font-size: 26rpx;
  font-weight: bold;
}

.bottom-left-center {
  text-align: center;
  height: 18rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.bottom-left-center-c {
  width: 18rpx;
  height: 18rpx;
  background: #fff;
  border-radius: 50%;
}

.bottom-right {
  width: 96rpx;
  height: 82rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  border-top-left-radius: 41rpx;
  border-bottom-left-radius: 41rpx;
}

.bottom-right-text {
  font-size: 28rpx;
  color: #FF6CA4;
}

.two {
  height: 80%;
  overflow-y: scroll;
  background-color: #FF6CA4;
  position: relative;
}

.two-float {
  position: absolute;
  right: 0;
  /* top: -50rpx; */
  width: 96rpx;
  height: 82rpx;
  background-color: #fff;
  border-radius: 50% 0 0 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 1;
}

.two-float-text {
  font-size: 22rpx;
  color: #FF6CA4;
}

.two-menu {
  width: 100%;
}

.menu-title {
  padding-left: 48rpx;
  color: #fff;
  font-size: 32rpx;
}

.menu-item-list {
  padding: 0 100rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.menu-item {
  width: 120rpx;
  margin-right: 95rpx;
  padding: 10rpx 0;
}

.menu-item:nth-child(3n){
  margin-right: 0;
}

.menu-item-icon {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-bottom: 10rpx;
}

.menu-item-text {
  display: block;
  text-align: center;
  font-size: 22rpx;
  color: #fff;
}

.date-back{
	background-color: rgba(0,0,0,0.2);
	width: 100vw;
	height: 100vh;
	position: absolute;
	top: 0;
	left: 0;
}
.date-wrap {
    z-index: 1000;
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 650rpx;
    height: 300rpx;
    border-radius: 50rpx;
    background: #fff;
    text-align: center;
	padding-top: 50rpx;
}


.date-btn-wrap {
    display: flex;
    margin-top: 74rpx;
    padding: 0 66rpx;
    font-size: 32rpx;
}

.date-cancel {
    flex: 1;
    width: 240rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 40rpx;
    text-align: center;
    border: 1rpx solid #FF6CA4;
    color: #FF6CA4;
    margin-right: 36rpx;
}

.date-cancel:active {
    opacity: .6;
}

.date-btn {
    flex: 1;
    width: 240rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 40rpx;
    text-align: center;
    border: 1rpx solid transparent;
    background: #FF6CA4;
    color: #fff;
}

.date-btn:active {
    opacity: .6;
}

.right-input {
	font-size: 36rpx;
	color: #333;
	width: 100%;
}
.show-view{
	display: block;
}
.hidder-view{
	display: none;
}