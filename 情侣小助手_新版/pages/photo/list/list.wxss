.album{
	width: 300rpx;
	height: 300rpx;
	display: inline-block;
	padding-left:50rpx ;
	padding-top:50rpx ;
	text-align: center;
}
.newImg{
	/*border: 5rpx #ffcccc solid; */
	width: 300rpx;
	height: 300rpx;
	display: inline-block;
}
.primary{
	width: 300rpx;
	height: 240rpx;
	display: inline-block;
}
.title{
	font-size: 28rpx;
	
}
.number{
	font-size: 20rpx;
}
.newSchedule{
	position: fixed;
	bottom: 50rpx;
	right: 50rpx;
	padding: 0rpx;
	margin: 0rpx;
	width:80rpx;
	height: 80rpx;
	border-radius:50%;
}
.butImg{
	position: relative;
	top:-2rpx;
	left: -2rpx;
	width: 85rpx;
	height: 85rpx;
}

/*列表*/
.mobileListBorder{
    padding-top:60rpx;
    background-image: url('https://img.yuzktyu.top/zSGF7ZKTy7SZ8MXsXMFYJQ4W.jpg');
    height: 100vh;
}
.main{
    display:flex;
    flex-wrap: wrap;
}
.subMain{
    width:50%;
    height:410rpx;
}
.subMainImage{
    position: relative;
    left:10rpx;
}
.downImage{
    position: absolute;
    height:360rpx;
    width:360rpx;
}
.upImage{
    position: absolute;
    width:360rpx;
    height:360rpx;
}
.images{
    position: absolute;
    width:300rpx;
    height:260rpx;
    top:50rpx;
    left:28rpx;
}
.collectImage{
    width:60rpx;
    height:60rpx;
    position: absolute;
    right:50rpx;
    top:40rpx;
}
.subMainText{
    text-align: center;
    font-size:28rpx;
    color:#b2b2b2;
    padding-top:360rpx; 
}
::-webkit-scrollbar{

width: 0;

height: 0;

color: transparent;

}
.tapTxt {
    text-align: center;
    font-size: 30rpx;
    line-height: 50rpx;
    color: #333;
    width: 100%;
}