.details{
	background-color: #fff2f2;
	padding:40rpx 30rpx;
	min-height: 100vh;
}
.note_details{
	background-color: #fff2f2;
}
.form_date{
	background-color:white ;
	margin: 0 0 40rpx 0;
	border-radius: 20rpx;
	padding-left: 20rpx;
	box-shadow: 16rpx 16rpx 6rpx #aaaaaa;
}
.dayName{
	padding: 20rpx 0;
	display: block;
	border-bottom: 1rpx solid #eee;
	height: 60rpx;
	font-size: 30rpx;
}
.date{
	padding: 20rpx 0;
	display: block;
	border-bottom:1rpx solid #eee ;
	margin-bottom:5rpx ;
	height: 60rpx;
	font-size:30rpx ;
}
.content{
	width: 100%;
}
.saveButton{
	color: #fff;
	background-color:#aaffcc;
	float: left;
	display: inline;
	height: 100rpx;
	width: 300rpx;
	margin-right:30rpx ;
}
.delButton{
	color: #fff;
	background-color: #ffcccc;
	float: right;
	display: inline;
	height: 100rpx;
	width: 300rpx;
	margin-left:30rpx ;
}
