page {
    background: #f5f5f5;
}

.container {
    padding: 22rpx 30rpx 40rpx;
}

.container>view {
    width: 100%;
}

.avatar {
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
    margin: 30rpx auto;
}

.tapTxt {
    text-align: center;
    font-size: 30rpx;
    line-height: 50rpx;
    color: #333;
    width: 100%;
}

.quesCont {
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.quesCont .tapTxt {
    text-align: left;
}

.quesCont .tapTxt:last-child {
    text-align: right;
    color: #1d82fe;
    font-size: 26rpx;
}

.recordBox {
    width: 100%;
    box-sizing: border-box;
}

.recordList {
    background: #fff;
    border-radius: 20rpx;
    margin: 20rpx 0 30rpx;
    box-sizing: border-box;
    padding: 0 20rpx 10rpx;
    margin-bottom: 30rpx;
    box-shadow: 0 2px 4px 0 rgba(0,0,0,0.10);
}

.createTime {
    height: 60rpx;
    line-height: 80rpx;
    font-size: 28rpx;
    color: #333;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.createTime .showQs {
    text-align: right;
    font-size: 26rpx;
    color: #666;
}

.friendInfo {
    position: relative;
    height: 130rpx;
    width: 100%;
    padding: 20rpx 0 20rpx 135rpx;
    box-sizing: border-box;
    border-bottom: 2rpx solid #ccc;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.recordList .friendInfo:last-child {
    border: none;
}

.friendPic {
    width: 125rpx;
    height: 80rpx;
    position: absolute;
    left: 0;
    top: 30rpx;
    box-sizing: border-box;
}

.friendPic text {
    display: inline-block;
    width: 35rpx;
    height: 35rpx;
    line-height: 35rpx;
    text-align: center;
    vertical-align: middle;
    margin-top: -3rpx;
    border-radius: 50%;
    background: #999;
    margin-right: 8rpx;
    color: #fff;
    font-size: 20rpx;
}

.friendPic text.firstScore {
    background: #fecb2b;
}

.friendPic text.secondScore {
    background: #fecccc;
}

.friendPic text.thirdScore {
    background: #6acdfd;
}

.friendPic image {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}

.leftLine,.rightLine {
    height: 100rpx;
    line-height: 45rpx;
    color: #333;
    font-size: 28rpx;
}

.leftLine view {
    color: #aaa;
}

.leftLine .friendName {
    color: #333;
    font-size: 30rpx;
    line-height: 55rpx;
}

.leftLine .friendName text:first-child {
    max-width: 64%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: inline-block;
    vertical-align: middle;
}

.leftLine .friendName .answerCount {
    color: red;
    font-size: 26rpx;
    margin-left: 10rpx;
    font-weight: 700;
}

.rightLine .compDegree {
    font-size: 40rpx;
    line-height: 100rpx;
}

.rightLine {
    line-height: 60rpx;
}

.rightLine .money {
    font-size: 26rpx;
}

.noAnswer {
    text-align: center;
    color: #aaa;
    font-size: 28rpx;
    line-height: 30rpx;
}

.noRecord {
    align-self: center;
    margin-top: 35%;
}

.noRecordImg {
    width: 330rpx;
    height: 260rpx;
    margin: 0 auto;
    display: block;
}

.noRecord .tapTxt {
    color: #999;
    font-size: 30rpx;
}

.friendAnswerPop {
    width: 600rpx;
    height: 800rpx;
    box-sizing: border-box;
    padding: 30rpx;
    border-radius: 20rpx;
    position: relative;
    top: 45%;
    left: 50%;
    margin: -400rpx 0 0 -300rpx;
    background: #fff;
}

.friendAnswerPop .close {
    position: absolute;
    display: block;
    right: 16rpx;
    top: -10rpx;
    font-size: 60rpx;
    color: #666;
    font-weight: 600;
}

.friendAnswerPop .headPic {
    display: block;
    margin-top: 0rpx;
    margin-left: 15rpx;
    width: 130rpx;
    height: 130rpx;
    border-radius: 50%;
    border: 2rpx solid #fff;
    margin-bottom: 4rpx;
}

.friendAnswerPop .friendName {
    color: #666;
    line-height: 50rpx;
    font-size: 32rpx;
    position: absolute;
    top: 35rpx;
    left: 210rpx;
    letter-spacing: 2rpx;
}

.friendAnswerPop .friendAnswerBox {
    width: 100%;
    box-sizing: border-box;
    height: 580rpx;
    padding: 10rpx 30rpx;
    overflow-x: hidden;
    overflow-y: scroll;
    -webkit-overflow-scrolling: touch;
    line-height: 40rpx;
    color: #333;
    font-size: 30rpx;
}

.friendAnswerBox .question {
    font-size: 30rpx;
    color: #6c5519;
    line-height: 45rpx;
}

.friendAnswerPop .friendAnswer {
    margin-bottom: 15rpx;
    padding: 10rpx 0 20rpx;
    border-bottom: 1rpx solid #f3f3f3;
}

.friendAnswerPop .answerBox {
    margin-top: 10rpx;
}

.friendAnswerPop .answerBox text {
    font-size: 28rpx;
    color: #333;
}

.friendAnswerPop .radioIcon {
    display: inline-block;
    vertical-align: middle;
    width: 36rpx;
    height: 36rpx;
    border-radius: 50%;
    overflow: hidden;
    box-sizing: border-box;
    background: #fff;
    margin-top: -5rpx;
    margin-right: 15rpx;
    border: none;
    background: url("data:image/png;base64,") no-repeat;
    background-size: 100% 100%;
}

.friendAnswerPop .isRight .radioIcon {
    background: url("data:image/png;base64,") no-repeat;
    background-size: 100% 100%;
}

.changePay {
    width: 300rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    background: red;
    color: #fff;
    border-radius: 10rpx;
    z-index: 1rpx;
    font-size: 26rpx;
    margin: 10rpx auto 0rpx auto;
}

.pay_group {
    width: 500rpx;
    height: 465rpx;
    background: #fff;
    top: 30vh;
    left: 125rpx;
    position: absolute;
    padding: 20rpx;
    border-radius: 10rpx;
    letter-spacing: 3rpx;
}

.pay_radio {
    margin: 20rpx 0 3rpx 0;
    font-size: 45rpx;
    color: red;
    width: 100%;
    display: block;
}

.radio_btn {
    background: #ffd944;
    width: 160rpx;
    height: 70rpx;
    font-size: 35rpx;
    bottom: -10rpx;
    line-height: 70rpx;
}

.radio_btn::after {
    border: none;
}

.radio_tip {
    font-size: 25rpx;
    margin-bottom: 20rpx;
    width: 410rpx;
    height: auto;
    line-height: auto;
    margin-left: 50rpx;
}

.radio_tit {
    background: #fff;
    width: 100%;
    height: 70rpx;
    font-size: 40rpx;
    line-height: 70rpx;
    text-align: center;
    border-bottom-color: #000;
}

.radio_cancel {
    position: absolute;
    width: 60rpx;
    height: 60rpx;
    right: -15rpx;
    top: -18rpx;
    font-weight: 500;
    font-size: 60rpx;
}

.getcorrect {
    width: 190rpx;
    height: 50rpx;
    position: absolute;
    line-height: 50rpx;
    text-align: center;
    font-size: 27rpx;
    left: 210rpx;
    top: 100rpx;
    border-radius: 8rpx;
    z-index: 1;
    letter-spacing: 2rpx;
}

.correct_btn1 {
    background: #FFD944;
    box-shadow: 0 8rpx 2rpx 0 rgba(219,182,33,1);
}

.correct_btn2 {
    background: #83dff3;
    box-shadow: 0 8rpx 2rpx 0 rgba(63,184,210,1);
}

.lockModal {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 600rpx;
    height: 390rpx;
    margin: -235rpx 0 0 -300rpx;
    background: #fff;
    border-radius: 20rpx;
    box-shadow: 0 0 5rpx #333;
    text-align: center;
    box-sizing: border-box;
}

.lockModal .close {
    font-size: 60rpx;
    position: absolute;
    right: 10rpx;
    top: -13rpx;
}

.lockModal .title {
    border-bottom: 0.5rpx solid #e1e1e1;
    line-height: 90rpx;
    letter-spacing: 3rpx;
    font-size: 35rpx;
}

.lockModal .text {
    padding: 25rpx 50rpx 40rpx;
    color: #999;
    text-align: left;
    font-size: 28rpx;
}

.lockModal .shareBtn {
    width: 300rpx;
    height: 90rpx;
    line-height: 90rpx;
    border-radius: 45rpx;
    margin: 0 auto;
    background: #ffd944;
    font-size: 32rpx;
    letter-spacing: 3rpx;
    border: none;
    outline: none;
}
.record-title{
    display: flex;
    flex-direction: row;
}
.avatar {
    width: 80rpx;
    height: 80rpx;
    border-radius: 40rpx;
    box-sizing: border-box;
    border: 4rpx solid #fff;
    margin: 5rpx 40rpx 5rpx 10rpx;
    overflow: hidden;
}
.havaAnswer {
    text-align: center;
    color: #ffcccc;
    font-size: 28rpx;
    line-height: 50rpx;
}