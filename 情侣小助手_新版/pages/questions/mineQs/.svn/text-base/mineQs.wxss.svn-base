.container {
    position: relative;
    padding: 0;
}

.bgImg {
    position: absolute;
    height: 100%;
    top: 0;
    left: 0;
    z-index: 1;
}

.content {
    position: relative;
    box-sizing: border-box;
    padding: 45rpx 45rpx 0;
    width: 100%;
    height: 100%;
    z-index: 2;
    overflow-x: hidden;
    overflow-y: scroll;
    -webkit-overflow-scrolling: touch;
}

.questionBox {
    width: 100%;
    box-sizing: border-box;
    background: #fff;
    border-radius: 10rpx;
    padding: 25rpx 30rpx;
    margin-bottom: 30rpx;
}

.question {
    font-size: 30rpx;
    color: #6c5519;
    line-height: 45rpx;
    margin-bottom: 10rpx;
}

.radioIcon {
    display: inline-block;
    vertical-align: middle;
    width: 38rpx;
    height: 38rpx;
    border-radius: 50%;
    overflow: hidden;
    box-sizing: border-box;
    background: #fff;
    margin-right: 15rpx;
    background: url("data:image/png;base64,") no-repeat;
    background-size: 100% 100%;
    border: none;
}

.questionBox text {
    font-size: 28rpx;
    line-height: 40rpx;
    margin: 10px 0;
    color: #333;
}

.dealBox {
    position: relative;
    z-index: 5;
    width: 100%;
    box-sizing: border-box;
    margin: 20rpx auto 0;
}

.dealBox .btn {
    margin: 0 auto 32rpx;
    width: 400rpx;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 32rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    box-shadow: 0 1px 4px 0 rgba(0,0,0,0.18);
    border-radius: 94px;
    border: none;
    color: #4a3c12;
    background: #fede18;
}